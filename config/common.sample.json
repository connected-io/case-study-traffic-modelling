{
  "arrivalProbability": 0.9,
  "durationInTicks": 10000,
  "numberOfIterations": 10,
  "simulationDefaults": {
    "laneChangeDelayTicks": 5,
    "lanePlacement": "random",
    "minTicksInLane": 2,
    "minTickSpeedupPercent": 60,
    "numberOfLanes": 3,
    "timeOnRoadFunction": "2 * Math.max(0, n - 5) + 20"
  },
  "output": {
    "format": "csv",
    "path": "./results",
    "columns": [
      [ "simulationTime", "Elapsed Time of Simulation" ],
      [ "carsEntered", "Number of Cars entered" ],
      [ "laneChangeDelayTicks", "Lane Change Delay" ],
      [ "minTicksInLane", "Min Time in Lane" ],
      [ "minTickSpeedupPercent", "Time Saved %" ],
      [ "numberOfLanes", "Number of Lanes" ],
      [ "averageTimeOnRoad", "Avg Time" ],
      [ "carsRemaining", "Cars Remain" ],
      [ "carsOnRoadAggregateTime", "Aggregate Time" ],
      [ "totalLaneChanges", "Number of Lane Changes" ],
      [ "overallThroughput", "Aggregate Throughput (cars / 100 ticks)" ],
      [ "averageThroughput", "Average Throughput per tick (cars / 100 ticks)" ]
    ]
  }
}
